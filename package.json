{
  "name": "xcaliber-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "scripts": {
    "start": "npm run build && npm run server",
    "start:dev": "npm run build:dev && npm run server",
    "start:test": "npm run build:test && npm run server",
    "start:prod": "npm run build:prod && npm run server",
    "build": "env UV_THREADPOOL_SIZE=100 NODE_ENV=local webpack --debug",
    "build:dev": "NODE_ENV=dev webpack",
    "build:test": "NODE_ENV=test webpack",
    "build:prod": "NODE_ENV=production webpack",
    "build:aot-old": "npm run clean:aot && ./node_modules/.bin/ngc -p src/tsconfig.server.aot.json && ./node_modules/.bin/ngc -p src/tsconfig.browser.aot.json && SERVER_TYPE=local-aot webpack --progress --colors",
    "build:aot": "SERVER_TYPE=local-aot webpack --progress --colors",
    "prebuild": "npm run clean",
    "clean:aot": "rimraf dist && rimraf src/aot-browser && rimraf src/aot-server",
    "clean": "rimraf dist",
    "test": "karma start",
    "server": "node --max-old-space-size=4096 dist/server.js",
    "server:dev": "nodemon dist/server.js",
    "watch": "env UV_THREADPOOL_SIZE=100 NODE_ENV=local webpack --watch",
    "webpack": "env UV_THREADPOOL_SIZE=100 NODE_ENV=local webpack",
    "watch:dev": "npm run watch",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@agm/core": "^1.0.0-beta.0",
    "@angular/animations": "^5.0.5",
    "@angular/cdk": "^5.0.4",
    "@angular/common": "~5.0.5",
    "@angular/compiler": "5.0.5",
    "@angular/core": "~5.0.5",
    "@angular/forms": "~5.0.5",
    "@angular/http": "~5.0.5",
    "@angular/material": "^5.0.4",
    "@angular/platform-browser": "~5.0.5",
    "@angular/platform-browser-dynamic": "~5.0.5",
    "@angular/platform-server": "~5.0.5",
    "@angular/router": "~5.0.5",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "compression": "^1.7.1",
    "core-js": "^2.5.1",
    "express": "^4.15.2",
    "http-proxy-middleware": "^0.17.4",
    "ie-shim": "^0.1.0",
    "install": "^0.10.1",
    "npm": "^5.2.0",
    "preboot": "^6.0.0-beta.1",
    "rxjs": "^5.3.0",
    "style-loader": "^0.18.2",
    "swiper": "^3.4.2",
    "xhr2": "^0.1.4",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.0.5",
    "@ngtools/webpack": "^1.8.5",
    "@types/express": "^4.0.39",
    "@types/jasmine": "^2.8.2",
    "@types/node": "^8.0.54",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.16",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "codelyzer": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "cssnext": "^1.8.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-replace-webpack-plugin": "^2.2.6",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.2.3",
    "style-ext-html-webpack-plugin": "^3.4.5",
    "tslint": "^5.5.0",
    "typescript": "^2.5.2",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0"
  }
}
